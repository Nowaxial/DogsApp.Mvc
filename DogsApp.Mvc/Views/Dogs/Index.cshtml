@model DogsApp.Mvc.Models.Dog[]
<!DOCTYPE html>
    <html>
    <head>
        <title>Dogs</title>
    </head>
    <body>
        <h1>Dogs</h1>
    <ul>
        @foreach (var dog in Model)
        {
            <li>@dog.Name 
                <button><a href="@dog.Id"> Delete</a></button>
                <button><a href="edit/@dog.Id">Edit</a></button>
            </li>
        }
    </ul>

    <a href="/create">Add dog</a>

    </body>
    </html>